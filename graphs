import networkx as nx
import matplotlib.pyplot as plt

nx.barbell_graph(4,2)
G=nx.barbell_graph(4,2)
nx.draw(G)

#complete_graphh
G=nx.complete_graph(4)
nx.draw(G)

#cycle_grap#complete_graphh
G=nx.cycle_graph(5)
nx.draw(G)

#ladder_graph
G=nx.ladder_graph(5)
nx.draw(G)

#path_graph
G=nx.path_graph(4)
nx.draw(G)

#star_graph
G=nx.star_graph(5)
nx.draw(G)

#wheel_graph
G=nx.wheel_graph(4)
nx.draw(G)

#random_graph
G=nx.gnp_random_graph(5,0.5)
nx.draw(G)





